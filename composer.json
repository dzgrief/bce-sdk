{
    "name": "dzgrief/bce-sdk",
    "description": "Baidu Cloud SDK for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "dzgrief",
            "email": "529265776@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^1.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Dzgrief\\Bce\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dzgrief\\Bce\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
